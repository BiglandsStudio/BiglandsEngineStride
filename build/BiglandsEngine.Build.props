<!-- Build file pre-included by all BiglandsEngine projects -->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <BiglandsEngineCommonPreSettingsName>BiglandsEngine</BiglandsEngineCommonPreSettingsName>
    <BiglandsEnginePlatforms Condition="'$(BiglandsEnginePlatforms)' == ''">Windows</BiglandsEnginePlatforms>
  </PropertyGroup>
  <PropertyGroup Condition="'$(BiglandsEnginePlatform)' == 'Windows'">
    <BiglandsEngineGraphicsApis Condition="'$(BiglandsEngineGraphicsApis)' == ''">Direct3D11</BiglandsEngineGraphicsApis>
  </PropertyGroup>
</Project>
